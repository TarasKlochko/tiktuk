{
  "extends": ["plugin:react/jsx-runtime", "airbnb", "airbnb/hooks", "prettier"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/function-component-definition": "off",
    "max-len": ["warn", { "code": 120 }]
  }
}
